{
  "name": "design_patterns/design_patterns",
  "description": "Basic php design patterns",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Dre Board",
      "email": "dreboard@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^8",
    "monolog/monolog": "1.23.*",
    "wazaari/monolog-mysql": ">1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Design_Patterns\\Observer\\": ["observer/classes", "observer/tests"],
      "Design_Patterns\\Decorator\\": ["decorator/classes", "decorator/tests"],
      "Design_Patterns\\Strategy\\": ["strategy/classes", "strategy/tests"],
      "Design_Patterns\\NullObject\\": ["null_object/classes", "null_object/tests"],
      "Design_Patterns\\Database\\": ["database/classes", "database/tests"],
      "Design_Patterns\\TemplateMethod\\": ["template_method/classes", "template_method/tests"],
      "Design_Patterns\\Singleton\\": ["Singleton/classes", "Singleton/tests"],
      "Design_Patterns\\ChainOfResponsibility\\": ["chain_of_responsibility/classes", "chain_of_responsibility/tests"],
      "Design_Patterns\\Facade\\": ["facade/classes", "facade/tests"]
    }
  }
}